# Multi-Tenant Configuration
REGISTRY_API_URL=http://localhost:3000
# Token para autenticação service-to-service com o Registry (obter credenciais de backend)
# Deve ser o mesmo token configurado no Registry (REGISTRY_SERVICE_TOKEN)
REGISTRY_SERVICE_TOKEN=your-secret-service-token-here
# Tenant padrão: use 'dev' para desenvolvimento local, 'advice' para produção
DEFAULT_TENANT_SLUG=dev

# Supabase Configuration (Fallback - usado quando tenant não especificado)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key

# AssemblyAI Configuration
ASSEMBLYAI_API_KEY=your-assemblyai-api-key

# OpenAI Configuration (para análise de transcrição)
OPENAI_API_KEY=your-openai-api-key

# Server Configuration
PORT=8002

# CORS Configuration
CORS_ORIGINS=http://localhost:5174,http://localhost:8000,http://localhost:3000
